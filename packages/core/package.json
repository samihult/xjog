{
  "name": "@samihult/xjog",
  "version": "0.0.315",
  "description": "XState chart runner for long-running charts",
  "engines": {
    "npm": ">=8.19.4",
    "yarn": "^1.22.8",
    "node": ">=16.x"
  },
  "keywords": [
    "statechart",
    "harel",
    "xstate"
  ],
  "repository": "https://github.com/samihult/xjog",
  "homepage": "https://www.xjog.io/",
  "author": "Sami Hult <sami.hult@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "lib",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.js --passWithNoTests",
    "lint": "eslint src/**/*.ts && prettier --check src",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "yarn lint && yarn build && echo 'should also run: yarn test + yarn check'",
    "clean": "rm -rf node_modules lib"
  },
  "dependencies": {
    "@samihult/xjog-core-persistence": "^0.0.37",
    "@samihult/xjog-util": "^0.0.32",
    "async-mutex": "^0.3.2",
    "rxjs": "^7.4.0",
    "uuid": "^8.3.2",
    "xstate": "4.26.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.223",
    "@swc/jest": "^0.2.22",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "concurrently": "^6.2.1",
    "cpx-fixed": "^1.6.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.0.3",
    "jest": "^28.1.2",
    "prettier": "^2.3.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "ansi-regex@>=5.0.0 <5.0.1": "5.0.1",
    "minimist": "^1.2.6",
