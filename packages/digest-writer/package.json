{
  "name": "@samihult/xjog-digest-writer",
  "version": "0.0.14",
  "description": "XJog chart digest writer",
  "engines": {
    "npm": ">=8.19.4",
    "yarn": "^1.22.8",
    "node": ">=16.x"
  },
  "keywords": [
    "statechart",
    "harel",
    "xjog"
  ],
  "repository": "https://github.com/samihult/xjog",
  "homepage": "https://www.xjog.io/",
  "author": "Sami Hult <sami.hult@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "lib",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "yarn build:bin ; yarn build:files",
    "build:bin": "tsc",
    "build:files": "cpx-fixed 'src/**/*.sql' lib",
    "prePublishOnly": "yarn build",
    "clean": "rm -rf node_modules lib"
  },
  "dependencies": {
    "pg": "^8.7.3",
    "pg-bind": "^1.0.1",
    "pg-listen": "^1.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cpx-fixed": "^1.6.0",
    "jest": "^28.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.7.4"
  }
}
